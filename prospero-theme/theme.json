{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "#007bff",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#6c757d",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#28a745",
					"name": "Tertiary"
				},
				{
					"slug": "highlight",
					"color": "#ffc107",
					"name": "Highlight"
				},
				{
					"slug": "text",
					"color": "#333333",
					"name": "Text"
				},
				{
					"slug": "dark-text",
					"color": "#f7f7f7",
					"name": "Dark Mode Text"
				},
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "dark-background",
					"color": "#2b2a33",
					"name": "Dark Background"
				}
			],
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"duotone": [],
			"gradients": []
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fontSizes": [
				{
					"slug": "small",
					"size": "0.875rem",
					"name": "Small"
				},
				{
					"slug": "normal",
					"size": "1rem",
					"name": "Normal"
				},
				{
					"slug": "medium",
					"size": "1.25rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.75rem",
					"name": "Large"
				},
				{
					"slug": "extra-large",
					"size": "2.5rem",
					"name": "Extra Large"
				}
			]
		},
		"spacing": {
			"units": ["rem", "em", "%"],
			"customSpacingSize": false,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"slug": "small",
					"size": "1rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "2rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "3rem",
					"name": "Large"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"blocks": {
			"core/button": {
				"color": {
					"custom": false,
					"customGradient": false
				},
				"typography": {
					"customFontSize": false
				},
				"border": {
					"radius": false
				}
			},
			"core/paragraph": {
				"color": {
					"custom": false
				},
				"typography": {
					"customFontSize": false
				}
			},
			"core/heading": {
				"color": {
					"custom": false
				},
				"typography": {
					"customFontSize": false
				}
			},
			"core/group": {
				"color": {
					"custom": false
				}
			},
			"core/columns": {
				"color": {
					"custom": false
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--text)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					}
				}
			},
			"button": {
				"color": {
					"background": "var(--prospero-btn-primary-bg, var(--wp--preset--color--primary))",
					"text": "var(--prospero-btn-primary-text, #ffffff)"
				},
				"border": {
					"radius": "var(--prospero-btn-primary-radius, 4px)"
				},
				"typography": {
					"fontWeight": "500"
				},
				":hover": {
					"color": {
						"background": "var(--prospero-btn-primary-hover-bg)",
						"text": "var(--prospero-btn-primary-hover-text)"
					}
				}
			}
		}
	}
}
